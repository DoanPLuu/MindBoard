@model IEnumerable<viduC4MVC.Models.Product>

<h2>Danh sách sản phẩm</h2>

<table border="1" cellpadding="5">
    <tr>
        <th>ID</th>
        <th>Tên sản phẩm</th>
        <th>Giá</th>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Id</td>
            <td>@p.Name</td>
            <td>@p.Price $</td>
        </tr>
    }
</table>
